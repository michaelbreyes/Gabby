<!DOCTYPE html>
<html lang="en" ng-app>
    <head>
        <meta charset="utf-8" />
        <title>Gabby - @ViewBag.Title</title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <link href="@System.Web.Optimization.BundleTable.Bundles.ResolveBundleUrl("~/Content/css")" rel="stylesheet" type="text/css" />
		<link href="@System.Web.Optimization.BundleTable.Bundles.ResolveBundleUrl("~/Content/themes/base/css")" rel="stylesheet" type="text/css" />
        <link href='http://fonts.googleapis.com/css?family=Droid+Sans:400,700|Droid+Serif:400,400italic|Inconsolata' rel='stylesheet'>
        <script src="@Url.Content("~/Content/Scripts/libs/jquery-1.7.2.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Content/Scripts/libs/jquery-ui-1.8.11.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Content/Scripts/Globals.js")" type="text/javascript"></script>  
        <script src="@Url.Content("~/Content/Scripts/libs/md5.js")" type="text/javascript"></script>
        <meta name="viewport" content="width=device-width" />
    </head>
	<body>
		<div id="contains-all" ng-controller="AngularCtrl">
			<div id="left-side" style="display: none;">@RenderSection("featured", required: false)</div><div id="middle">@RenderBody()</div><div id="right-side">@RenderSection("rightside", required: false)</div>
			@RenderSection("script", required: false)
		</div>
	</body>
</html>
